<h1>投稿一覧</h1>

<% @books.each do |book| %>

  <p>タイトル</p>
  <span><%= book.title %></span>
  
<% end %>
<%= form_with model: @book, url: '/books', method: :post do |f| %>

  <h4>タイトル</h4>
  <%= f.text_field :title%>

  <h4>本文</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book'%>

<% end %>
